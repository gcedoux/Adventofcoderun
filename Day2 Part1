var boxId = ["ababab","abcccd"];
var boxIdFirstSplit = boxId[0].split("");
var newBoxId = [];


for (i = 0; i < boxIdFirstSplit.length; i++){
if (newBoxId.includes(boxIdFirstSplit[i]) == false){
newBoxId.push(boxIdFirstSplit[i])
}
}


document.write(newBoxId);


