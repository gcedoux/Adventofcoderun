var startF = [+3, +3, +4, -2, -4];
var arr = [];
var s = 0;//holds the frequency
var i = 0;


while (i < startF.length) {
	s+=startF[i];
	if (arr.includes(s) === true){
	 document.write("match found with number " + s);
	 break;
	 } else {document.write("");
	 arr.push(s);
   startF.push(startF[i]);
	 }
	 i++
	 }
	 
	 startF.push(startF[i]);
